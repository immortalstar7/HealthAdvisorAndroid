<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn2crack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />
    <!-- Allow to connect with internet and to know the current network state -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.learn2crack.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.learn2crack.Register"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.learn2crack.Registered"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.learn2crack.Main"
            android:label="Learn2Crack USER PANEL" >
        </activity>
        <activity
            android:name="com.learn2crack.PasswordReset"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.learn2crack.ChangePassword"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.learn2crack.library.Profile"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.learn2crack.sensefall.TabBarExample"
            android:label="@string/app_name" >
>
        </activity>
        <activity android:name="com.learn2crack.sensefall.ReadAccelData" />
        <activity android:name="com.learn2crack.sensefall.Prediction" >
        </activity>
        <activity android:name="com.learn2crack.sensefall.MainActivity" />
        <activity android:name="com.learn2crack.sensefall.DisplayMessageActivity" />
        <activity android:name="com.learn2crack.sensefall.LoadContacts" />
        <activity
            android:name="com.learn2crack.sensefall.Configure"
            android:configChanges="orientation|screenLayout"
            android:label="@string/title_activity_configure"
            android:parentActivityName="com.learn2crack.sensefall.LoadContacts"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".sensefall.FallDetection" />
        </activity>
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.learn2crack.GraphActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <uses-permission android:name="android.permission.INTERNET" />
        </activity>
    </application>

</manifest>